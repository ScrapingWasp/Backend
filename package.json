{
    "scripts": {
        "build": "npm run build:ts && npm run build:assets",
        "build:ts": "cross-env NODE_OPTIONS=--max-old-space-size=4096 ./node_modules/typescript/bin/tsc -p tsconfig.json",
        "build:assets": "copyfiles -u 1 \"./assets/**/*\" \"./dist/assets\"",
        "test": "jest --coverage",
        "prod": "npm run build && cross-env NODE_ENV=production node -r newrelic ./dist/server.js",
        "prod:built": "cross-env NODE_ENV=production node -r newrelic ./dist/server.js",
        "start": "cross-env NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=development ts-node-dev --respawn --debug --stack-trace-limit=10000 index.js"
    },
    "dependencies": {
        "aws-sdk": "^2.1472.0",
        "axios": "^1.5.1",
        "body-parser": "^1.20.2",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "crawlee": "^3.5.7",
        "dotenv": "^16.3.1",
        "dynamoose": "^3.2.1",
        "express": "^4.18.2",
        "helmet": "^7.0.0",
        "morgan": "^1.10.0",
        "playwright": "^1.38.1",
        "puppeteer": "^21.3.8",
        "redis": "^4.6.10",
        "stripe": "^13.10.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@aws-sdk/client-secrets-manager": "^3.332.0",
        "@babel/preset-env": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/express-rate-limit": "^6.0.0",
        "@types/http-auth": "^4.1.1",
        "@types/jest": "^29.5.0",
        "@types/multer": "^1.4.7",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "babel-jest": "^29.5.0",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.38.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "lint-staged": "^13.2.1",
        "prettier": "^2.7.1",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.4"
    }
